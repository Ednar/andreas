{% extends "admin/admin_base.twig" %}


{% block content %}
    
    <a href="index.php?AdminAddPrint/addPrint/">Add new print</a>
    
    <div class="listing-print">

    {% for print in prints %}

            <img src="img/{{ print.thumbnail }}">
            <a href="index.php?AdminEditPrint/editPrint/{{ print.printID }}">Edit</a>
            <a href="index.php?AdminDeletePrint/deletePrint/{{ print.printID }}">Delete</a>

    {% endfor %}
    
    </div>

{% endblock %}
